<h1>Listing dres</h1>

<table>
  <tr>
    <% @dres.first.attributes.each do |attributes| %>
      <th><%= attributes%></th>
    <% end %>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @dres.each do |dre| %>
  <tr>
    <% dre.attributes.each do |attribute| %>
      <td><%= dre.send(attribute) %></td>
    <% end %>
    <td><%= link_to 'Show', dre_path(dre.id) %></td>
    <td><%= link_to 'Edit', edit_dre_path(dre.id) %></td>
    <td><%= link_to 'Destroy', dre_path(dre.id), confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Dre', new_dre_path %>
