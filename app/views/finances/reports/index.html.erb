<table class="styled table table-striped table-hover table-bordered">
  <caption>Reports</caption>
  <tr>
    <th>Nome</th>
    <th>Descrição</th>
    <th></th>
  </tr>
  <% @reports.each do |report| %>
    <tr>
      <td>
      <%= report.name %>
      </td>
      <td>
      <%= report.description %>
      </td>
      <td>
        <%= link_to 'Show', report_path(report.name) %>
      </td>
    </tr>
  <% end %>
</table>

<%= link_to 'Novo Relatório', new_report_path %>

